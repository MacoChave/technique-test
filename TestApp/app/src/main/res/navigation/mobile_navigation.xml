<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_country">

    <fragment
        android:id="@+id/navigation_country"
        android:name="com.macochave.testapp.ui.country.CountryFragment"
        android:label="@string/title_country"
        tools:layout="@layout/fragment_country" />

    <fragment
        android:id="@+id/navigation_department"
        android:name="com.macochave.testapp.ui.department.DepartmentFragment"
        android:label="@string/title_department"
        tools:layout="@layout/fragment_department" />

    <fragment
        android:id="@+id/navigation_person"
        android:name="com.macochave.testapp.ui.person.PersonFragment"
        android:label="@string/title_person"
        tools:layout="@layout/fragment_person" />
